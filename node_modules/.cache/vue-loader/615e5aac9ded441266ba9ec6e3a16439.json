n\n        <h5 class=\"pb-5\">Input fields</h5>\n\n        <!--Grid row-->\n        <div class=\"row\">\n          <!--Grid column-->\n          <div class=\"col-md-4 mb-4\">\n            <mdb-input type=\"text\" label=\"Basic example\" />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-md-4 mb-4\">\n            <mdb-input size=\"sm\" type=\"text\" label=\"Small input\" />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-md-4 mb-4\">\n            <mdb-input\n              type=\"text\"\n              label=\"Example label\"\n              placeholder=\"Placeholder\"\n            />\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <!--Grid row-->\n        <div class=\"row\">\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <mdb-input type=\"text\" label=\"Example label\" value=\"John Doe\" />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <mdb-input type=\"text\" label=\"Basic example\" disabled />\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <!--Grid row-->\n        <div class=\"row text-left\">\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <!--Email validation-->\n            <mdb-input type=\"text\" label=\"Type your email\" icon=\"envelope\" />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <!--Password validation-->\n            <mdb-input type=\"password\" label=\"Type your password\" icon=\"lock\" />\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <h5 class=\"pb-5\">Textarea</h5>\n\n        <!--Grid row-->\n        <div class=\"row text-left\">\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <!--Basic textarea-->\n            <mdb-textarea :rows=\"5\" label=\"Basic textarea\" />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <!--Textarea with icon prefix-->\n            <mdb-textarea :rows=\"5\" label=\"Icon prefix\" icon=\"pencil-alt\" />\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <h5 class=\"pb-5\">Checkboxes and Radio</h5>\n\n        <!--Grid row-->\n        <div class=\"row\">\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-12 mb-4\">\n            <mdb-input\n              type=\"checkbox\"\n              id=\"checkbox1\"\n              label=\"Classic checkbox\"\n            />\n            <mdb-input\n              type=\"checkbox\"\n              id=\"checkbox2\"\n              label=\"Filled-in checkbox\"\n              filled\n            />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-6 mb-4\">\n            <mdb-input\n              type=\"radio\"\n              id=\"option1-1\"\n              name=\"option1\"\n              value=\"option1\"\n              label=\"Option 1\"\n              checked\n              @input=\"handleRadio\"\n            />\n            <mdb-input\n              type=\"radio\"\n              id=\"option1-2\"\n              name=\"option1\"\n              value=\"option2\"\n              label=\"Option 2\"\n              @input=\"handleRadio\"\n            />\n            <mdb-input\n              type=\"radio\"\n              id=\"option1-3\"\n              name=\"option1\"\n              value=\"option3\"\n              label=\"Option 3\"\n              @input=\"handleRadio\"\n            />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-6 mb-4\">\n            <!-- Switch -->\n            <mdb-switch />\n\n            <!-- Disabled Switch -->\n            <mdb-switch disabled />\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <h5 class=\"pb-5\">File input</h5>\n\n        <!--Grid row-->\n        <div class=\"row\">\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <mdb-file-input btnColor=\"primary\" />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-md-6 mb-4\">\n            <mdb-file-input\n              multiple\n              textFieldTitle=\"Upload your files\"\n              btnColor=\"primary\"\n            ></mdb-file-input>\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <h5 class=\"pb-5\">Range</h5>\n\n        <!--Grid row-->\n        <div class=\"row\">\n          <!--Grid column-->\n          <div class=\"col-md-12 mb-4\">\n            <mdb-range-input :min=\"0\" :max=\"100\" :value=\"50\"></mdb-range-input>\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <h5 class=\"pb-5\">Select</h5>\n\n        <!--Grid row-->\n        <div class=\"row mb-5\">\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-12 mb-4\">\n            <!--Name-->\n            <mdb-select\n              @getValue=\"getSelectValue\"\n              label=\"Choose your option\"\n              :options=\"[\n                { value: 'Option 1', text: 'Option nr 1' },\n                { value: 'Option 2', text: 'Option nr 2' },\n                { value: 'Option 3', text: 'Option nr 3' },\n                { value: 'Option 4', text: 'Option nr 4' },\n                { value: 'Option 5', text: 'Option nr 5' }\n              ]\"\n            />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-6 mb-4\">\n            <!--Name-->\n            <mdb-select\n              color=\"primary\"\n              @getValue=\"getSelectValue\"\n              label=\"Choose your option\"\n              :options=\"[\n                {\n                  value: 'Option 1',\n                  text: 'Option nr 1',\n                  disabled: true,\n                  selected: true\n                },\n                { value: 'Option 2', text: 'Option nr 2' },\n                { value: 'Option 3', text: 'Option nr 3' },\n                { value: 'Option 4', text: 'Option nr 4' },\n                { value: 'Option 5', text: 'Option nr 5' }\n              ]\"\n            />\n          </div>\n          <!--Grid column-->\n\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-6 mb-4\">\n            <!--Name-->\n            <mdb-select\n              multiple\n              search\n              @getValue=\"getSelectValue\"\n              label=\"Choose your option\"\n              :options=\"[\n                { value: 'Option 1', text: 'Option nr 1' },\n                { value: 'Option 2', text: 'Option nr 2' },\n                { value: 'Option 3', text: 'Option nr 3' },\n                { value: 'Option 4', text: 'Option nr 4' },\n                { value: 'Option 5', text: 'Option nr 5' }\n              ]\"\n            />\n          </div>\n          <!--Grid column-->\n        </div>\n        <!--Grid row-->\n\n        <!--Grid row-->\n        <div class=\"row\">\n          <!--Grid column-->\n          <div class=\"col-lg-4 col-md-12 mb-4\">\n            <!--Name-->\n            <mdb-select\n              @getValue=\"getSelectValue\"\n              :options=\"[\n                {\n                  value: 'team 1',\n                  text: 'team 1',\n                  disabled: true,\n                  optgroup: true\n                },\n                { value: 'Option 1', text: 'Option nr 1', selected: true },\n                { value: 'Option