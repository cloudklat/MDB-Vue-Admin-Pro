nputs-->\n\n    <!--Section: Docs link-->\n    <section class=\"pb-5\">\n      <!--Panel-->\n      <div class=\"card text-center\">\n        <h3 class=\"card-header primary-color white-text\">Full documentation</h3>\n        <div class=\"card-body\">\n          <p class=\"card-text\">\n            Read the full documentation for these components.\n          </p>\n          <a\n            href=\"https://mdbootstrap.com/docs/vue/forms/inputs/\"\n            target=\"_blank\"\n            class=\"btn btn-primary\"\n            >Learn more</a\n          >\n        </div>\n      </div>\n      <!--/.Panel-->\n    </section>\n    <!--Section: Docs link-->\n  </mdb-container>\n</template>\n\n<script>\nimport {\n  mdbContainer,\n  mdbInput,\n  mdbTextarea,\n  mdbSelect,\n  mdbSwitch,\n  mdbFileInput,\n  mdbRangeInput\n} from \"mdbvue\";\n\nexport default {\n  name: \"Basic\",\n  components: {\n    mdbContainer,\n    mdbInput,\n    mdbTextarea,\n    mdbSelect,\n    mdbSwitch,\n    mdbFileInput,\n    mdbRangeInput\n  },\n  data() {\n    return {\n      radio1: 0,\n      radio2: 0,\n      radio3: 0,\n      active1: true,\n      active2: false,\n      active3: false\n    };\n  },\n  methods: {\n    checkboxChange(val) {\n      console.log(val);\n    },\n    handleRadio(val) {\n      console.log(val);\n    },\n    toggleActiveState1() {\n      this.active1 = !this.active1;\n    },\n    toggleActiveState2() {\n      this.active2 = !this.active2;\n    },\n    toggleActiveState3() {\n      this.active3 = !this.active3;\n    },\n    toggleRadio1() {\n      this.radio1 = 1;\n    },\n    toggleRadio2() {\n      this.radio1 = 2;\n    },\n    toggleRadio3() {\n      this.radio1 = 3;\n    },\n    toggleRadio4() {\n      this.radio2 = 1;\n    },\n    toggleRadio5() {\n      this.radio2 = 2;\n    },\n    toggleRadio6() {\n      this.radio2 = 3;\n    },\n    toggleRadio7() {\n      this.radio3 = 1;\n    },\n    toggleRadio8() {\n      this.radio3 = 2;\n    },\n    toggleRadio9() {\n      this.radio3 = 3;\n    },\n    getSelectValue(value) {\n      console.log(value);\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped></style>\n"]}]}